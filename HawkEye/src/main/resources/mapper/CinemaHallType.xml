<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hawkeye.mapper.CinemaHallTypeMapper">
    <select id="getSpecialHalls" resultType="CinemaHallType">
        SELECT `id`,`type_name`,`is_special` FROM `cinema_hall_type` WHERE `is_special` = 1;
    </select>
</mapper>